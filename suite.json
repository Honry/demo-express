{
    "pkg-blacklist": [
        "config.xml",
        "pack.py",
        "tests.*.xml",
        "demo-*.png",
        "manifest.json",
        "suite.json",
        "inst.*"
    ],
    "pkg-list": {
        "apk": {
            "blacklist": [
                "*",
                ".git"
            ],
            "copylist": {
                "res": "res",
                "samples-wrt/ManifestCSP/res/resources": "res",
                "samples/CSP-Meta/resources": "csp-meta/resources",
                "LICENSE": "LICENSE",
                "inst.apk.py": "inst.py"
            },
            "subapp-list": {
                ".": {
                    "app-name": "demoexpress",
                    "apk-type": "MANIFEST",
                    "blacklist": [
                        "*",
                        ".git"
                    ],
                    "copylist": {
                        "manifest.json": "manifest.json",
                        "css": "css",
                        "fonts": "fonts",
                        "demo-ex.png": "icon.png",
                        "index.html": "index.html",
                        "js": "js",
                        "res": "res",
                        "samples": "samples",
                        "samples-wrt": "samples",
                        "tests.android.xml": "tests.xml",
                        "tests_list.html": "tests_list.html",
                        "VERSION": "VERSION"
                    }
                },
                "samples-wrt/SchemesCheck/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestDemo1/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestDemo2/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestDemo3/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestDemo4/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestDemo5/res": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/SchemeContent/res": {
                    "apk-type":"MANIFEST"
                },
                "samples/Viewport/ViewportTest": {
                    "apk-type":"MANIFEST"
                },
                "samples-wrt/ManifestCSP/res": {
                    "apk-type":"MANIFEST",
                    "copylist": {
                        "../../../res/media/w3c/movie_300.mp4": "resources/media/w3c/movie_300.mp4",
                        "../../../js/jquery-2.1.3.min.js": "resources/js/jquery.js"
                    }
                },
                "samples-wrt/ManifestDemo7/res": {
                    "apk-type":"MANIFEST"
                }
            }
        },
        "embeddingapi": {
            "blacklist": [
                "*"
            ], 
            "copylist": {
                "inst.em.py": "inst.py", 
                "tests.embedding.xml": "tests.xml"
            }, 
            "subapp-list": {
                "samples-embedding": {
                    "app-name": "embeddingapi-demo", 
                    "app-type": "EMBEDDINGAPI", 
                    "embeddingapi-library-name": "crosswalk-webview",
                    "copylist": {
                        "../demo-ex-em.png": "res/drawable-hdpi/icon.png"
                    }
                }
            }
        },
        "cordova4.x": {
            "blacklist": [
                "*",
                ".git"
            ],
            "copylist": {
                "LICENSE": "LICENSE",
                "inst.apk.py": "inst.py"
            },
            "subapp-list": {
                ".": {
                    "app-name": "webapi-cordova-demo",
                    "apk-type": "MANIFEST",
                    "blacklist": [
                        "*",
                        ".git"
                    ],
                    "copylist": {
                        "extra_plugins/cordova-admob-4.x": "PACK-TOOL-ROOT/extra_plugins/cordova-admob-4.x",
                        "css": "css",
                        "fonts": "fonts",
                        "demo-ex-co.png": "icon.png",
                        "index.html": "index.html",
                        "js": "js",
                        "res": "res",
                        "samples": "samples",
                        "samples-cordova/cordova-incl.js": "cordova-incl.js",
                        "samples-cordova": "samples",
                        "tests.android.xml": "tests.xml",
                        "tests_list.html": "tests_list.html",
                        "VERSION": "VERSION"
                    }
                }
            }
        }
    },
    "pkg-name": "DemoExpress"
}
